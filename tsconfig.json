{
    "compilerOptions": {
      "target": "es2019",
      "module": "commonjs",
      "esModuleInterop": true,
      "sourceMap": true,
      "strict": false,
      "noImplicitAny": false,
      "moduleResolution": "node",
      "outDir": "./dist",
      "baseUrl": "./src",
      "paths": {
        "*": ["*", "/*"]
      }
    },
    "include": ["/*.ts"],
    "exclude": ["node_modules", "**/*.spec.ts"]
  }
  